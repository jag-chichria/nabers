							{
								"source": {
									"name": "RowTimestamp",
									"physicalType": "timestamp"
								},
								"sink": {
									"name": "prod_timestamp",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "RatingHeaderID",
